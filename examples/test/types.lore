/**
 * This file contains definitions for Least Upper Bound and Subtyping/Fit tests.
 */

abstract class Animal
abstract class Bird extends Animal
abstract class Mammal extends Animal
abstract class Fish extends Animal
class Chicken extends Bird
class Penguin extends Bird
class Raven extends Bird
class Human extends Mammal
class Cat extends Mammal
class ScottishFold extends Cat
class Unicorn extends Mammal
class Goldfish extends Fish

label Status
label Healthy extends Status
label Sick extends Status

abstract class Wheel
class CoolWheel extends Wheel
class CheapWheel extends Wheel
abstract class Engine
class GasEngine extends Engine
class ElectricEngine extends Engine

abstract entity Car {
  component Wheel
  component Engine
}

abstract entity Cycle {
  component Wheel
}

abstract entity Motorcycle extends Cycle {
  component Engine
}

abstract entity Bicycle extends Cycle
