function create(x: Any, y: Any): (Any, Any) = (x, y)

action consume(tuple: (Any, Any)) {
  println('Consume any and any!')
}
action consume(tuple: (String, Int)) {
  println('Consume string and int!')
}
action consume(tuple: (Boolean, String)) {
  println('Consume boolean and string!')
}

action test() {
  consume(create('string', 15))
  consume(create(true, 'string'))
  consume(create('string', 'string'))
}
