function double(list: [Real]): [Real] = {
  for (e <- list) {
    e * 2
  }
}
action testDouble() {
  let list = [1, 2, 3, 4]
  let result = double(list)
  println('Doubled list: $result')
}

function combinations(list: [Real]): [(Real, Real)] = {
  for (a <- list, b <- list) {
    (a, b)
  }
}
action testCombinations() {
  let list = [1, 2, 3, 4]
  let result = combinations(list)
  println('Combinations: $result')
}

action test() {
  testDouble()
  testCombinations()
}
