func lowIncrement(n: Int): Int = if n < 25 then n + 1 else n

func towards25(n: Int): Int = do
  if n < 25 then       n + 1
  else if n > 25 then  n - 1
  else                 n
end

func abc(a: Boolean, b: Boolean, c: Boolean): Boolean = do
  if a
    return b
  end else
    return c
  end
end

func dangle(flag: Boolean, flag2: Boolean): Int = do
  if flag then if flag2 then return 1 else return 2
  3
end

func test(): [Boolean | Int] = [
  if true then false else true,
  abc(true, false, true),
  abc(false, false, true),

  lowIncrement(20),
  lowIncrement(25),
  towards25(25),
  towards25(23),
  towards25(30),

  dangle(true, true),
  dangle(true, false),
  dangle(false, true),
  dangle(false, false)
]
