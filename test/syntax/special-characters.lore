trait Foo?
struct Bar?(yes?: Boolean) extends Foo?

func truly?(foo?: Foo?): Boolean
func truly?(bar?: Bar?): Boolean = bar?.yes?

func truly?(shape: %{ no?: Boolean }): Boolean = !shape.no?

func test(): [Boolean] = [
  truly?(Bar?(true)),
  truly?(Bar?(false)),
  truly?(%{ no?: true }),
  truly?(%{ no?: false })
]
