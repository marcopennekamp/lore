trait A
trait B extends A
struct C extends B
trait D extends B
struct E extends D

func stringify(a: A): String = 'A'

func stringify(b: B): String = {
  let super = stringify.fixed[A](b)
  concat(super, 'B')
}

func stringify(c: C): String = {
  let super = stringify.fixed[B](c)
  concat(super, 'C')
}

func stringify(d: D): String = {
  let super = stringify.fixed[B](d)
  concat(super, 'D')
}

func stringify(e: E): String = {
  let super = stringify.fixed[D](e)
  concat('E', super)
}

func test(): [String] = [
  stringify(C()),
  stringify(E()),
  stringify.fixed[B](C()),
  stringify.fixed[D](E()),
  stringify.fixed[A](C()),
  stringify.fixed[A](E())
]
