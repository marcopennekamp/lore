func low_increment(n: Int): Int = if n < 25 then n + 1 else n

func towards_25(n: Int): Int = cond
  n < 25 => n + 1
  n > 25 => n - 1
  true   => n
end

func abc(a: Boolean, b: Boolean, c: Boolean): Boolean = do
  if a
    return b
  else
    return c
  end
end

func dangle(flag: Boolean, flag2: Boolean): Int = do
  if flag then if flag2 then return 1 else return 2
  3
end

func dangle2(condition1: Boolean, condition2: Boolean): Int = do
  if condition1
    if condition2 then return 1
  else
    return 2
  end
  3
end

func test(): [Boolean | Int] = [
  if true then false else true,
  abc(true, false, true),
  abc(false, false, true),

  low_increment(20),
  low_increment(25),
  towards_25(25),
  towards_25(23),
  towards_25(30),

  dangle(true, true),
  dangle(true, false),
  dangle(false, true),
  dangle(false, false),

  dangle2(true, true),
  dangle2(true, false),
  dangle2(false, true),
  dangle2(false, false),
]
