struct Position(x: Real = 0.0, y: Real = 0.0, z: Real = 0.0)

func from_2D(x: Real, y: Real): Position = do
  Position { x, y }
end

func from_1D(x: Real): Position = do
  from_2D(x, 0.0)
end


func test(): [Position] = [
  Position(1.0, 2.0, 3.0),
  Position { y = 5.0 },
  from_2D(20.0, 10.0),
  from_1D(7.0),
]
