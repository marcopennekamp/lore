use lore.number.floor

func pow(x: Real, exp: Int): Real = do
  let mut e = exp
  let mut result = 1.0
  while e > 0
    result *= x
    e -= 1
  end
  result
end

func test(): [Real] = [
  pow(0.5, 0),
  pow(0.5, 2),
  pow(2.0, 4),
  pow(3.0, 2),
  pow(0.0, 5),
  pow(pow(2.0, 2), 2),
  pow(pow(2.0, floor(pow(2.0, 2))), 2),
]
