use lore.List

func print_list(list: [A]): String where A = 'Generic list: $list'
func print_list(list: [Number]): String = 'Number list: $list'
func print_list(list: [String | Number]): String = 'String/Number list: $list'
func print_list(list: [String | Number | Boolean]): String = 'String/Number/Boolean list: $list'

func test(): [String] = do
  let mut results: [String] = []

  let list = [10]
  results = results :+ print_list(list)

  let merged = List.concat(list, ['test'])
  results = results :+ print_list(merged)

  let merged2 = List.concat(merged, [true, 'wow'])
  results = results :+ print_list(merged2)

  let merged3 = List.concat(merged2, [15])
  results = results :+ print_list(merged3)

  results
end
