use lore.Enum.[filter, map]
use lore.number.remainder

@where A
func filter_curried(f: A => Boolean): [A] => [A] = do
  list => filter(list, f)
end

func test(): [[Int]] = do
  let data = [
    [1, 2, 3, 4, 5],
    [4, 8, 11, 15, 6],
    [],
    [1, 1, 1, 1, 1],
  ]
  data |> map(filter_curried(x => remainder(x, 2) == 0))
end
