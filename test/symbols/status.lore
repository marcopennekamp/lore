type Status = #ok | #error

func stringify(status: Status): String
func stringify(status: #ok): String = 'Success!'
func stringify(status: #error): String = 'Failure!'

func process(value: Int): (Status, String) = {
  if (value < 5) (#ok, 'Below five.')
  else (#error, 'At or above five.')
}

func stringify(result: (Status, String)): String = stringify(first(result))

func test(): [String] = [
  stringify(process(3)),
  stringify(process(5)),
  stringify(process(2)),
  stringify(process(10))
]
