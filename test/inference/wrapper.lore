struct Wrapper[-A, +B] { function: A => B }

func mapWrapper(list: [A], wrapper: Wrapper[B, C]): [C] where A, B >: A, C = map(list, wrapper.function)

func test(): [String] = {
  let numbers = [1, 2, 3]
  mapWrapper(
    numbers,
    Wrapper(n => {
      let strings = repeat(n, 'Hey!')
      stringJoin(strings, ' ')
    })
  )
}
