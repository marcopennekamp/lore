struct Wrapper[-A, +B](function: A => B)

@where A, B >: A, C
func map_wrapper(list: [A], wrapper: Wrapper[B, C]): [C] = map(list, wrapper.function)

func test(): [String] = do
  let numbers = [1, 2, 3]
  map_wrapper(
    numbers,
    Wrapper(n => do
      repeat('Hey!', n) |> string_join(' ')
    end)
  )
end
