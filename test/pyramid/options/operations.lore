use lore.core.to_string
use lore.Enum.[empty?, map, present?]
use lore.Option
use lore.Option._

func test(): [String] = do
  let a = Some('hello options')
  let b = None
  let c: Option[String] = Some('test')
  let d: Option[String] = None

  [
    get(a, 'fallback'),
    get(b, 'fallback'),
    get(c, 'fallback'),
    get(d, 'fallback'),

    empty?(a),
    empty?(b),
    empty?(c),
    empty?(d),

    present?(a),
    present?(b),
    present?(c),
    present?(d),
  ] |> map(to_string)
end
