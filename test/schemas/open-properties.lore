struct Box[+A] {
  open value: A
}

trait Content
struct Juice extends Content
struct Berry extends Content
struct Hammer extends Content

func toString(box: Box[Juice]): String = 'juice box'
func toString(box: Box[Berry]): String = 'berry box'
func toString(thing: %{ value: Content }): String = 'thing with content'
func toString(thing: %{ value: Hammer }): String = 'thing with hammer'

// This verifies that struct types with open properties are correctly interned. Open property types should be taken
// into account.
func test(): [String] = {
  let boxes = [Box[Content](Juice()), Box[Content](Berry()), Box[Content](Hammer())]
  map(boxes, toString)
}
