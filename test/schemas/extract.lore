trait Crate[+A]
func extract(crate: Crate[A]): A where A

struct IntCrate extends Crate[Int] { value: Int }
func extract(crate: IntCrate): Int = crate.value

struct ListCrate[+A] extends Crate[[A]] { list: [A] }
func extract(crate: ListCrate[A]): [A] where A = crate.list

struct TobyCrate extends Crate[String]
func extract(crate: TobyCrate): String = 'I am a crate and my name is Toby!'


func test(): [String] = [
  toString(extract(IntCrate(5))),
  toString(extract(ListCrate([1, 2, 3]))),
  extract(TobyCrate())
]
