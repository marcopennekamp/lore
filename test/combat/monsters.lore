trait Monster extends Combatant

struct Voidbringer extends Monster {
  stats: Stats = Stats {
    health = 30,
    maximumHealth = 30,
    regeneration = 2,
    dodge = 0.2,
    attack = 5,
    speed = 2
  }

  statusEffects: StatusEffects = StatusEffects([])
}

func name(voidbringer: Voidbringer): String = 'Voidbringer'

act enterCombat(voidbringer: Voidbringer) {
  buff(voidbringer, createArmored(2, 3))
}
