use lore.Enum.map

func numbers(): [Int] = [1, 2, 3, 4, 5] |> map(v => v + 3)

func names(): [String] = [
  %{ name: 'Alpha', foo: 23, baz: 10 },
  %{ name: 'Beta', foo: 5, bar: 34 },
  %{ name: 'Gamma', foo: 7, bar: 12, baz: 12 },
] |> map(thing => thing.name)

func test(): ([Int], [String]) = (numbers(), names())
